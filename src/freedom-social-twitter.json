{
  "name": "freedom-social-twitter",
  "description": "Twitter social provider for freedom.js",
  "app": {
    "script": [
      "freedom-social-twitter.js",
      "twitter-auth.js"
    ]
  },
  "default": "social",
  "provides": [
    "social"
  ],
  "permissions": [
    "core.oauth",
    "core.view",
    "core.xhr"
  ],
  "views": {
    "twitter-login": {
      "main": "twitter-view.html",
      "files": [ "twitter-view.js" ]
    }
  }
}
